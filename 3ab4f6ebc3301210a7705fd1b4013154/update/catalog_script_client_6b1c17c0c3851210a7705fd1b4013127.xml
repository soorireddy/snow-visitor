        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
   if (isLoading || newValue == '') {
      return;
   }
   var type = newValue;
   if(type == 'walkin'){
		g_form.setDisplay('health_questionarre',true);
		g_form.setDisplay('host_details',false);
   }else if(type == 'scheduled'){
		g_form.setDisplay('host_details',true);
		g_form.setDisplay('health_questionarre',false);
   }else if(type == 'group'){
		g_form.setDisplay('visitor_details_group',true);
		g_form.setDisplay('host_details',false);
		g_form.setDisplay('health_questionarre',false);
   }else{
        g_form.setReadOnly('end_date',true);
