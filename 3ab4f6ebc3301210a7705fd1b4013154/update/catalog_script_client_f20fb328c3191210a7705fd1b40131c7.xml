        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') {
        return;
    }
    g_form.setDisplay('passport', false);
    g_form.setDisplay('qr_code_scanner', false);
    g_form.setDisplay('otp', false);
	var type = newValue;
    if (type == 'PASSPORT' || type == 'DL' || type == 'PHOTO' || type == 'SSN') {
        g_form.setDisplay('passport', true);
    } else if (type == 'QR') {
        g_form.setDisplay('qr_code_scanner', true);
    } else if (type == 'OTP') {
        g_form.setDisplay('otp', true);
    }
}]]></script>
